{
  "name": "zkx402-agent-auth-ui",
  "version": "1.0.0",
  "description": "Web UI for zkX402 Agent Authorization with Zero-Knowledge ML",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "vite",
    "build": "vite build",
    "build:jolt": "cd ../jolt-atlas-fork/zkml-jolt-core && cargo build --release --example proof_json_output",
    "start": "node server.js",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "ethers": "^6.15.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "lucide-react": "^0.263.1",
    "multer": "^1.4.5-lts.1",
    "onnxruntime-node": "^1.16.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ],
    "collectCoverageFrom": [
      "server.js",
      "src/**/*.{js,jsx}"
    ]
  }
}
